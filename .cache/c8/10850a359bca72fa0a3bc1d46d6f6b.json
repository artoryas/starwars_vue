         .then((body) => {\r\n                this.data = body.results;\r\n                this.isLoading = false;\r\n            });\r\n    },\r\n    components:{\r\n        ItemList,\r\n        SpeciesView,\r\n        SearchBar,\r\n        Pagination\r\n    },\r\n    methods:{\r\n        showSpecies(item){\r\n            this.isLoadingItem = true;\r\n            if(item.url.charAt(4) === ':'){\r\n                item.url = item.url.slice(0,4) + 's' + item.url.slice(4);\r\n            }\r\n            fetch(item.url)\r\n                .then(res => res.json())\r\n                .then(body => {\r\n                    this.species = body;\r\n                    this.isLoadingItem = false;\r\n            });\r\n        },\r\n        searchSpecies(value){\r\n            this.isLoading = true;\r\n            fetch(`https://swapi.dev/api/species/?search=${value}`)\r\n                .then((res) => res.json())\r\n                .then((body) => {\r\n                    this.data = body.results;\r\n                    this.isLoading = false;\r\n            });\r\n        },\r\n        showPage(page){\r\n            this.isLoading = true;\r\n            fetch(`https://swapi.dev/api/species/?page=${page}`)\r\n                .then((res) => res.json())\r\n                .then((body) => {\r\n                    this.data = body.results;\r\n                    this.isLoading = false;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>"},"lineCount":null}},"error":null,"hash":"798b4beb0b7dffb12a27b5cde52bdbb0","cacheData":{"env":{}}}