n                this.data = body.results;\r\n                this.isLoading = false;\r\n            });\r\n    },\r\n    components:{\r\n        ItemList,\r\n        PlanetView,\r\n        SearchBar,\r\n        Pagination\r\n    },\r\n    methods:{\r\n        showPlanet(item){\r\n            this.isLoadingItem = true;\r\n            if(item.url.charAt(4) === ':'){\r\n                item.url = item.url.slice(0,4) + 's' + item.url.slice(4);\r\n            }\r\n            fetch(item.url)\r\n                .then(res => res.json())\r\n                .then(body => {\r\n                    this.planet = body;\r\n                    this.isLoadingItem = false;\r\n            });\r\n        },\r\n        searchPlanet(value){\r\n            this.isLoading = true;\r\n            fetch(`https://swapi.dev/api/planets/?search=${value}`)\r\n                .then((res) => res.json())\r\n                .then((body) => {\r\n                    this.data = body.results;\r\n                    this.isLoading = false;\r\n            });\r\n        },\r\n        showPage(page){\r\n            this.isLoading = true;\r\n            fetch(`https://swapi.dev/api/planets/?page=${page}`)\r\n                .then((res) => res.json())\r\n                .then((body) => {\r\n                    this.data = body.results;\r\n                    this.isLoading = false;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"78ac9f59f65915706782a343f952fa82","cacheData":{"env":{}}}